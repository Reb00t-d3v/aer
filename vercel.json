{
  "rewrites": [
    {
      "source": "/api/images/(.*)",
      "destination": "/api/images/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/$1",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-for"
        }
      ]
    }
  ]
}
